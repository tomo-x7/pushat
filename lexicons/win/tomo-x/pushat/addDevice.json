{
	"lexicon": 1,
	"id": "win.tomo-x.pushat.addDevice",
	"defs": {
		"main": {
			"type": "procedure",
			"input": {
				"encoding": "application/json",
				"schema": {
					"type": "object",
					"required": ["token", "name"],
					"properties": {
						"token": {
							"type": "string"
						},
						"name": {
							"type": "string",
							"maxGraphemes": 30,
							"maxLength": 300
						}
					}
				}
			},
			"output": {
				"encoding": "application/json",
				"schema": {
					"type": "object",
					"required": ["id"],
					"properties": {
						"id": {
							"type": "string",
							"format": "tid"
						}
					}
				}
			}
		}
	}
}
