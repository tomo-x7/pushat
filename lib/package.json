{
	"name": "@tomo-x/pushat",
	"version": "0.0.4",
	"description": "PushAt SDK",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./signature": {
			"import": "./dist/signature.js",
			"types": "./dist/signature.d.ts"
		},
		"./digest": {
			"import": "./dist/digest.js",
			"types": "./dist/digest.d.ts"
		}
	},
	"bin": {
		"pushat-gen": "./dist/cli.js"
	},
	"type": "module",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"prepublishOnly": "pnpm run build",
		"gen": "lex gen-api ./src/lexicons/ ../lexicons/win/tomo-x/pushat/*.json ../lexicons/com/atproto/**/*.json"
	},
	"keywords": [
		"PushAt",
		"atproto",
		"push notification"
	],
	"author": "tomo-x",
	"license": "MIT",
	"devDependencies": {
		"@atproto/lex-cli": "^0.9.5",
		"@types/node": "^24.6.1",
		"typescript": "^5.9.3"
	},
	"files": [
		"dist",
		"src"
	],
	"dependencies": {
		"@atproto/lexicon": "^0.5.1",
		"@atproto/xrpc": "^0.7.5",
		"buffer": "^6.0.3",
		"citty": "^0.1.6"
	}
}
