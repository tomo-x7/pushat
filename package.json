{
	"name": "pushat",
	"version": "1.0.0",
	"private": true,
	"license": "MIT",
	"packageManager": "pnpm@10.17.1",
	"scripts": {
		"check": "biome check --write",
		"web": "pnpm --filter ./web",
		"main": "pnpm --filter ./main",
		"client": "pnpm --filter ./packages/client",
		"gen": "pnpm web run gen && pnpm main run gen && pnpm client run gen",
		"deploy": "wrangler deploy --minify",
		"dev": "wrangler dev",
		"cf-typegen": "wrangler types ./main/src/worker.d.ts --env-interface CloudflareBindings"
	},
	"engines": {
		"pnpm": ">=10.17.1",
		"node": ">=22.12"
	},
	"pnpm": {
		"executionEnv": {
			"nodeVersion": "22.19.0"
		}
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.3",
		"wrangler": "^4.38.0"
	}
}
